include <stdio.h>  
int main()  
{  
    int n;  
    int sum=0;  
    printf("Enter a number");  
    scanf("%d",&n);  
    int k=n;  
    int r;  
    while(k!=0)  
    {  
        r=k%10;  
        int f=fact(r);  
        k=k/10;  
        sum=sum+f;  
    }  
    if(sum==n)  
    {  
        printf("Number is a strong\n");  
    }  
    else  
    {  
        printf("Number is not a strong\n");  
    }  
    return 0;  
}
