#include<stdio.h>

int main()
{
  int age,till;
  printf("ENTER YOUR AGE\n");
  scanf("%d",&age);
  printf("PERSON IS ELIGIBLE FOR VOTING ONLY IF HIS/HER AGE IS 18 OR ABOVE ONLY\n");
  
  if(age>=18)
  {
      printf("YOU ARE ELIGIBLE FOR VOTING");
  }
  else if(age<=18 && age>=1)
  {
      till=18-age;
      printf("YOU ARE ALLOWED TO VOTE AFTER %d YEARS",till);
  }
  else 
  {
      printf("INPUT IS NOT CORRECT");
  }
  return 0;
}
