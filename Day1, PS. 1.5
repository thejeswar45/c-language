#include<stdio.h>
#include<math.h>
int main()
{
	int X,N,art;
	printf("1.power\n 2.addition\n 3.subraction\n 4.multiplication\n 5.division\n");
	printf("enter X value:");
	scanf("%d",&X);
	printf("enter N value:");
	scanf("%d",&N);
	printf("enter your choice:");
	scanf("%d",&art);
	switch(art)
	{
		case 1:
			printf("power of %d and %d is %d",X,N,X^N);
			break;
		case 2:
			printf("sum of %d and %d is %d",X,N,X+N);
			break;
		case 3:
			printf("subraction of %d and %d is %d",X,N,X-N);
			break;
		case 4:
			printf("multiplication of %d and %d is %d",X,N,X*N);
			break;
		case 5:
			printf("division of %d and %d is %d",X,N,X/N);
	}
	return 0;
}
